#include <iostream>
#include <climits>
using namespace std;

int LinearSearch(int arr[],int n)
{
   for ( int i=0;i<7;i++)
   {
       if (arr[i]==n)
          { return i;}
        
    
   }
   return -1;
       
    
}
int main() 
{  int arr[7];
   for ( int i=0;i<7;i++)
   {
       cout<<"Enter Elemenet for index ["<<i<<"] :";
       cin>>arr[i];
   }
   int n;
   cout<<"Enter no to find:";
   cin>>n;
  cout<<"Found at Index:" <<LinearSearch(arr,n);
    



   return 0;
}
